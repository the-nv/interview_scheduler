<h1>New Schedule</h1>

<strong>Candidate Details</strong><br>
<%= form_with scope: :candidate, url: candidates_path, local: true do |form| %>
    <p>
        <%= form.label :Name %>
        <%= form.text_field :name %>
    </p>

    <p>
        <%= form.label :Email %>
        <%= form.text_field :email %>
    </p>

    <%= form.submit :proceed %>

<% end %>